{
    "runtime": {
        "version": "Lua 5.1"
    },
    "hint": {
        "setType": true,
        "semicolon": "Disable"
    },
    "codeLens.enable": false,
    "diagnostics": {
        "unusedLocalExclude": [
            "_*"
        ],
        "ignoredFiles": "Disable",
        "libraryFiles": "Disable",
        "workspaceEvent": "OnSave",
        "groupFileStatus": {
            "strong": "Any",
            "strict": "Any",
            "type-check": "Any",
            "luadoc": "Any",
            "unused": "Any",
            "unbalanced": "Any",
            "redefined": "Any",
            "ambiguity": "Any",
            "duplicate": "Any",
            "codestyle": "None"
        },
        "groupSeverity": {
            "strict": "Error",
            "unbalanced": "Error",
            "redefined": "Error",
            "strong": "Error",
            "type-check": "Error",
            "ambiguity": "Error",
            "unused": "Warning",
            "duplicate": "Error",
            "luadoc": "Warning",
            "codestyle": "Information"
        }
    },
    "workspace": {
        "library": [
            "annotations.lua"
        ],
        "useGitIgnore": true
    },
    "diagnostics.globals": [
        "_G",
        "LibStub",
        "Settings",
        "InterfaceAddOnsList_Update",
        "InterfaceOptionsFrameAddOns",
        "InterfaceOptionsFrame_OpenToCategory",
        "OptionsListButtonToggle_OnClick",
        "GetBuildInfo",
        "C_AddOns",
        "PlaySound",
        "SOUNDKIT",
        "char",
        "WOW_PROJECT_ID",
        "WOW_PROJECT_MAINLINE",
        "WOW_PROJECT_CLASSIC"
    ]
}